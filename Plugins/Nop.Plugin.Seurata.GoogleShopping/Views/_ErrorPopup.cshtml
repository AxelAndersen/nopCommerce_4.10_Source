@model System.Collections.Generic.List<KeyValuePair<string, string>>

<ul style="list-style-type: none" class="listerror">
    @foreach (var data in Model)
    {
        if(data.Value == "error" || data.Value == "critical")
        {
            @: <li><svg version='1.1' width='24px' height='24px' viewBox='0 0 24 24' style='pointer-events: none;'><path d='M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z' fill='#c53929'></path></svg><span class='issues-span'>@data.Key</span></li>
        }
        else
        {
            @: <li><span class="issues-span-none">@data.Key</span></li>
        }
    }
    @if(Model.Count == 0)
    {
        <span>Please open feed to view variant wise error(s).</span>
    }
</ul>

